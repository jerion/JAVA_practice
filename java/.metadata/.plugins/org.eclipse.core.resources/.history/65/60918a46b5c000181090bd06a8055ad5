
public class P31 
{
	public static void main(String[] args)
	{
		System.out.println("8+9等於"+(8+9));
		System.out.println("17*17等於"+(17*17));
		
		int a1 = 8;
		int a2 = 9;
		int sad = a1 + a2;
		
		System.out.println("a1的值: "+a1);
		System.out.println("a2的值: "+a2+"\n");
		
		System.out.println("a1+a2的值: "+sad);
		
		a2 = a2 + 3;
		
		System.out.println("a2加3後為: "+a2);
	}
}
